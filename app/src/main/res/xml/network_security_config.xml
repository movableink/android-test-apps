<network-security-config>
    <base-config>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
        </trust-anchors>
    </base-config>

    <domain-config>
        <domain includeSubdomains="true">fanatics.xtremepush.com</domain>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="@raw/fanatics" />
        </trust-anchors>
    </domain-config>

    <debug-overrides>
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
            <certificates src="@raw/fanatics" />
        </trust-anchors>
    </debug-overrides>

</network-security-config>